<?xml version="1.0" encoding="utf-8"?>
<resources>

	<item type="id" name="mapcontainer" />

	<item type="id" name="secondary_icon" />

	<!-- Card ids -->

	<!-- This view will help dictate the desired height of a card if its width is set.
	     Ideally should be a RatioFrameLayout or RatioImageView -->
	<item type="id" name="card_height_regulator" />

	<!-- The main container of a card. Should be a ViewGroup -->
	<item type="id" name="card_main_container" />

	<!-- The main image associated with a card. Should be an ImageView -->
	<item type="id" name="card_image" />

	<!-- The name associated with a card. Should be a TextView -->
	<item type="id" name="card_name" />

	<!-- The description associated with a card. Should be a TextView -->
	<item type="id" name="card_description" />

	<!-- The container holding the card's avatar area. Should be a ViewGroup -->
	<item type="id" name="card_avatar_container" />

	<!-- The primary avatar associated with a card. Should be an ImageView -->
	<item type="id" name="card_avatar" />

	<!-- The username associated with an avatar in a card. Should be a TextView -->
	<item type="id" name="card_avatar_username" />

</resources>